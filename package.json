{
  "name": "lazy-reducer-hoc",
  "version": "0.0.1",
  "description": "react HOC that loads reducer on demand",
  "main": "lib/main.js",
  "repository": "git@gh-lxynox:lxynox/lazy-reducer.git",
  "author": "lxynox <lxynox@gmail.com>",
  "license": "MIT",
  "xo": {
    "extends": "xo-react",
    "esnext": true,
    "space": true,
    "semicolon": false,
    "env": [
      "browser"
    ]
  },
  "scripts": {
    "lint": "xo",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --open",
    "dev:example": "cross-env NODE_ENV=development webpack-dev-server --config example/webpack.config.js --hot --inline --open",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-export-default": "7.0.0-alpha.20",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.6.1",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.4",
    "webpack-dev-server": "^3.1.3",
    "xo": "^0.18.2",
    "react-redux": "^5.0.7"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "redux": "^4.0.0"
  }
}
